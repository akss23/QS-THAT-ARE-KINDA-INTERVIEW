class Solution {
public:
    bool uniqueOccurrences(vector<int>& arr) {
        int n=arr.size();
        map<int,int> mpp;
        for(int i=0;i<n;i++){
            mpp[arr[i]]++;
        }
        set<int> s;
        for(auto it: mpp){
            s.insert(it.second);
        }
        return s.size()==mpp.size();
    }
};
